{

  "version": 2,

  "builds": [

    {

      "src": "index.html",
      "src": "main.html",
      "src": "pair.html",
      "src": "qr.html",

      "use": "@vercel/static"

    }

  ],

  "routes": [

    { "src": "/(.*)", "dest": "/index.html" }
    { "src": "/(.*)", "dest": "/main.html" }
    { "src": "/(.*)", "dest": "/pair.html" }
    { "src": "/(.*)", "dest": "/qr.html" }

  ]

}
