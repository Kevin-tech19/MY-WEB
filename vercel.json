{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "main.html", "use": "@vercel/static" },
    { "src": "pair.html", "use": "@vercel/static" },
    { "src": "qr.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/main", "dest": "/main.html" },
    { "src": "/pair", "dest": "/pair.html" },
    { "src": "/qr", "dest": "/qr.html" }
  ]
}
